        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/login_kakao_cl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/login_img"
            android:layout_marginTop="19dp">

            <ImageButton
                android:id="@+id/login_kakao_btn"
                android:layout_width="361dp"
                android:layout_height="54.15dp"
                android:background="@drawable/complete_btn_rectangle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/login_kakao_ic"
                android:layout_width="21.66dp"
                android:layout_height="20.46dp"
                android:src="@drawable/ic_kakao"
                app:layout_constraintStart_toStartOf="@id/login_kakao_btn"
                android:layout_marginStart="18.1dp"
                app:layout_constraintTop_toTopOf="@id/login_kakao_btn"
                app:layout_constraintBottom_toBottomOf="@id/login_kakao_btn"
                />

            <TextView
                android:id="@+id/login_kakao_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/notosans_kr"
                android:text="카카오로 시작하기"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/login_kakao_btn"
                app:layout_constraintEnd_toEndOf="@id/login_kakao_btn"
                app:layout_constraintTop_toTopOf="@id/login_kakao_btn"
                app:layout_constraintBottom_toBottomOf="@id/login_kakao_btn"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


